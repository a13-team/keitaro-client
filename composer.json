{
    "name": "playtini/keitaro-client",
    "type": "library",
    "description": "Keitaro Client: Click API, Admin API",
    "keywords": ["api", "keitaro", "tds"],
    "license": "GPL-3.0-only",
    "require": {
        "php": "^8.1",
        "symfony/http-client": "^6.1|^7.0",
        "symfony/http-foundation": "^6.1|^7.0",
        "gupalo/dateutils": "^1.13",
        "psr/cache": "^3.0",
        "gupalo/json": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Playtini\\KeitaroClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Playtini\\KeitaroClient\\Tests\\": "tests/"
        }
    }
}
